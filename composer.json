{
    "name": "i2crm/whatsapp-stream-crypto",
    "description": "PSR-7 stream decorators for WhatsApp-style encryption/decryption",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "ext-openssl": "*",
        "guzzlehttp/psr7": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "I2CRM\\WSC\\": "src/"
        }
    },
    "authors": [
        {
            "name": "you",
            "email": "you@example.com"
        }
    ],
    "bin": [
        "bin/encrypt",
        "bin/decrypt",
        "bin/sidecar",
        "bin/test-sources"
    ],
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-deprecation-rules": "^2.0"
    }
}
